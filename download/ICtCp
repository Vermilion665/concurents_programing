                  <mtd>
                    <mi>P</mi>
                  </mtd>
                </mtr>
                <mtr>
                  <mtd>
                    <mi>T</mi>
                  </mtd>
                </mtr>
              </mtable>
              <mo>]</mo>
            </mrow>
          </mrow>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>P</mi>
            <mi>Q</mi>
            <mi>C</mi>
            <mn>2</mn>
          </mrow>
        </msub>
        <mo>=</mo>
        <msup>
          <mrow>
            <mo>(</mo>
