                <mi>N</mi>
              </mfrac>
            </mrow>
            <mrow>
              <mo>(</mo>
              <mrow>
                <mi>n</mi>
                <mo>+</mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mfrac>
                    <mn>1</mn>
                    <mn>2</mn>
                  </mfrac>
                </mrow>
                <mo>+</mo>
                <mrow class="MJX-TeXAtom-ORD">
                  <mfrac>
                    <mi>N</mi>
                    <mn>2</mn>
                  </mfrac>
                </mrow>
              </mrow>
              <mo>)</mo>
            </mrow>
            <mrow>
