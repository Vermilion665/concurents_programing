                          <mi>ln</mi>
                          <mo>&#x2061;<!-- ⁡ --></mo>
                          <mo stretchy="false">(</mo>
                          <mi>A</mi>
                          <mo stretchy="false">)</mo>
                          <mo stretchy="false">)</mo>
