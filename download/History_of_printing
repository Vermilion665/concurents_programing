en1985\"] = 1,\n    [\"CITEREFNoriyuki_Kasai\"] = 1,\n    [\"CITEREFPan1997\"] = 1,\n    [\"CITEREFPike,_A._W._G.Hoffmann,_D._L.García-Diaz,_M.Pettitt,_P._B.2012\"] = 1,\n    [\"CITEREFPolenz,_Peter_von.1991\"] = 1,\n    [\"CITEREFRoy_A._Allan2001\"] = 1,\n    [\"CITEREFSansom1961\"] = 1,\n    [\"CITEREFSeibt2016\"] = 1,\n    [\"CITEREFTaylorTaylor1995\"] = 1,\n    [\"CITEREFThomas_Christensen2007\"] = 1,\n    [\"CITEREFTsien1985\"] = 1,\n    [\"CITEREFTwitchett1998b\"] = 1,\n    [\"CITEREFWiles2012\"] = 1,\n    [\"CITEREFWilkinson2012\"] = 1,\n}\ntemplate_list = table#1 {\n    [\"Citation\"] = 7,\n    [\"Citation needed\"] = 4,\n    [\"Cite DCB\"] = 1,\n    [\"Cite book\"] = 21,\n    [\"Cite encyclopedia\"] = 1,\n    [\"Cite journal\"] = 4,\n    [\"Cite news\"] = 2,\n    [\"Cite web\"] = 21,\n    [\"Clarify\"] = 1,\n    [\"Convert\"] = 1,\n    [\"DEFAULTSORT:History Of Printing\"] = 1,\n    [\"Div col\"] = 1,\n    [\"Div col end\"] = 1,\n    [\"Harvnb\"] = 1,\n    [\"History of printing\"] = 1,\n    [\"IPAc-en\"] = 1,\n    [\"ISBN\"] = 10,\n    [\"Lang-ko\"] = 1,\n    [\"Main\"] = 25,\n    [\"Nihongo\"] = 1,\n    [\"Nihongo3\"] = 1,\n    [\"Page needed\"] = 1,\n    [\"Quotation\"] = 8,\n    [\"Reflist\"] = 1,\n    [\"See also\"] = 3,\n    [\"Sfn\"] = 48,\n    [\"Short description\"] = 1,\n    [\"Transliteration\"] = 6,\n    [\"US patent\"] = 1,\n    [\"Verify source\"] = 1,\n    [\"Webarchive\"] = 14,\n}\narticle_whitelist = table#1 {\n}\n"},"cachereport":{"origin":"mw1385","timestamp":"20230528165355","ttl":1814400,"transientcontent":false}}});});</script>
