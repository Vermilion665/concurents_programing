              <mtr>
                <mtd>
                  <mn>1</mn>
                </mtd>
                <mtd>
                  <mo>&#x2212;<!-- − --></mo>
                  <mn>1</mn>
                </mtd>
                <mtd>
                  <mn>1</mn>
                </mtd>
